<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".create_restaurant_form">
    <TextView
        android:id="@+id/titulo"
        android:layout_width="155dp"
        android:layout_height="36dp"
        android:text="@string/crear_restaurante"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="51dp"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/nombre_texto"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:importantForAutofill="no"
        android:inputType="textPersonName"
        android:text="@string/nombre"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="107dp"
        tools:ignore="LabelFor,MissingConstraints" />

    <EditText
        android:id="@+id/nit_texto"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:importantForAutofill="no"
        android:inputType="textPersonName"
        android:text="@string/nit"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="167dp"
        tools:ignore="LabelFor,MissingConstraints" />

    <EditText
        android:id="@+id/propietario_texto"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:autofillHints=""
        android:ems="10"
        android:inputType="textPersonName"
        android:text="@string/propietario"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="226dp"
        tools:ignore="LabelFor,MissingConstraints" />

    <EditText
        android:id="@+id/calle_texto"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:autofillHints=""
        android:ems="10"
        android:inputType="textPersonName"
        android:text="@string/calle"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="282dp"
        tools:ignore="LabelFor,MissingConstraints" />


    <TextView
        android:id="@+id/lugares"
        android:layout_width="155dp"
        android:layout_height="36dp"
        android:text="@string/lugar"
        android:textSize="32sp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="320dp"
        tools:ignore="MissingConstraints" />

    <com.google.android.gms.maps.MapView
        android:id="@+id/select_place"
        android:layout_width="408dp"
        android:layout_height="204dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="380dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/crear_restaurante"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/crear"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="636dp"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>